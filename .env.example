OPENAI_API_KEY=sk-...

# Mem0 graph configuration
MEM0_GRAPH_PROVIDER=neo4j  # or memgraph
NEO4J_URL=bolt://localhost:7687
NEO4J_USER=neo4j
NEO4J_PASSWORD=password

# Optional: Mem0 vector store (Qdrant)
MEM0_VECTOR_PROVIDER=qdrant
QDRANT_HOST=localhost
QDRANT_PORT=6333
# For Qdrant Cloud:
# QDRANT_URL=https://<your-cluster>.qdrant.io
# QDRANT_API_KEY=...
# QDRANT_USE_HTTPS=true

# Optional: Mongo/Vector infra (future)
